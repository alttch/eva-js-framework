{
  "name": "@eva-ics/framework",
  "version": "0.5.0",
  "description": "JavaScript/TypeScript Framework for EVA ICS",
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc src/*.ts --declaration --emitDeclarationOnly --declarationDir dist/ && mv dist/lib.d.ts dist/framework.es.d.ts",
    "preview": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alttch/eva-js-framework.git"
  },
  "main": "./dist/framework.umd.js",
  "module": "./dist/framework.es.js",
  "files": ["dist"],
  "types": "./dist/framework.es.d.ts",
  "exports": {
    ".": {
      "import": "./dist/framework.es.js",
      "require": "./dist/framework.umd.js"
    }
  },
  "keywords": [
    "iot",
    "iiot",
    "browser",
    "framework",
    "evaics",
    "eva-ics"
  ],
  "author": "Altertech",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/alttch/eva-js-framework/issues"
  },
  "dependencies": {
    "@altertech/jsaltt": "0.1.11"
  },
  "homepage": "https://www.eva-ics.com/",
  "devDependencies": {
    "typescript": "^5.1.3",
    "vite": "^4.3.9"
  }
}
